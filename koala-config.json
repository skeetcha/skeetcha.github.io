{
	"mappings": [
		{
			"src": "./scss",
			"dest": "./css"
		},
		{
			"src": "./listen/scss",
			"dest": "./listen/css"
		},
		{
			"src": "./about/scss",
			"dest": "./about/css"
		},
		{
			"src": "./buy/scss",
			"dest": "./buy/css"
		},
		{
			"src": "./connect/scss",
			"dest": "./connect/css"
		}
	],
	"ignores": ["*.css"],
	"options": {
		"outputStyle": "nested",
		"lineComments": false,
		"debugInfo": false,
		"sourceMap": false,
		"unixNewlines": false,
        "autoprefix": false,
        "autoprefixConfig" : "> 1%, last 2 versions, Firefox ESR, Opera 12.1"
	},
	"customOptions": [],
	"includePaths": [],
	"requireLibs": []
}